{
  "name": "nbf",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "rmdir /Q /S cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "cypress:open:qa": "npm run cypress:open --env platform=qa",
    "cypress:open:localhost": "npm run cypress:open --env platform=localhost",
    "cypress:open": "cypress open",
    "sanity": "npm run pretest && npm run cypress:run:qa:sanity",
    "cypress:run:qa:sanity": "cypress run --env platform=qa --spec 'cypress/integration/sanity/*.ts' || npm run posttest",
    "cypress:run": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports --charts true -i true",
    "posttest": "npm run combine-reports && npm run generate-report",
    "playwright:test:headed": "yarn playwright test --headed"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@playwright/test": "^1.14.1",
    "@types/cypress-cucumber-preprocessor": "^1.14.1",
    "@types/node": "^14.0.14",
    "cypress": "^4.5.0",
    "cypress-cucumber-preprocessor": "^2.5.0",
    "cypress-multi-reporters": "^1.4.0",
    "mocha": "^7.1.2",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^5.1.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "typescript-map": "0.0.7",
    "webpack": "^4.43.0",
    "xlsx": "^0.16.2"
  },
  "dependencies": {
    "cross-env": "^7.0.2"
  }
}
